<sbml xmlns="http://www.sbml.org/sbml/level2" version="1" level="2" xmlns:html="http://www.w3.org/1999/xhtml">
	<model id="test" name="test">
		<listOfUnitDefinitions>
			<unitDefinition id="mmol_per_gDW_per_hr" name="">
			</unitDefinition>
		</listOfUnitDefinitions>
		<listOfCompartments>
			<compartment id="c" name ="cytosol" />
			<compartment id="p" name ="periplasm" />
		</listOfCompartments>
		<listOfSpecies>
			<species id="A-cpd" name="A';" compartment="c" boundaryCondition="true" formula="formulaA" mass="10" >
			</species>
			<species id="B-cpd" name="B" compartment="p" boundaryCondition="true" formula="formulaB" mass="10d" >
			</species>
			<species id="C-cpd" name="C" compartment="c" boundaryCondition="true" formula="formulaC" mass="12.5" >
			</species>
			<species id="D-cpd" name="D" compartment="c" boundaryCondition="true" />
			
		</listOfSpecies>
		<listOfReactions>
			<reaction id="r1-rxn" name="R1" reversible="false" ec="1.1.1.1">
				<enzyme id="e1">
					<protein id="gene1_TH" name ="gene1 (TH)">
						<gene id="gene1" name="gene1" />
					</protein>
				</enzyme>
				<pathway id ="pathway1" name="pathway;" />
				<listOfReactants>
					<speciesReference species="A-cpd"/>
				</listOfReactants>
				<listOfProducts>
					<speciesReference species="C-cpd"/>
				</listOfProducts>
				<kineticLaw>
					<math xmlns="http://www.w3.org/1998/Math/MathML">
						<ci> FLUX_VALUE </ci>
					</math>
					<listOfParameters>
						<parameter id="LOWER_BOUND" value="-1000.0" units="mmol_per_gDW_per_hr"/>
						<parameter id="UPPER_BOUND" value="2.0" units="mmol_per_gDW_per_hr"/>
					</listOfParameters>
				</kineticLaw>
			</reaction>
			
			<reaction id="r2-rxn" name="R2&lt;i&gt;ase&lt;/i&gt;" reversible="false">
				<pathway id ="pathway3" name="pathway3" />
				<listOfReactants>
					<speciesReference species="D-cpd"/>
				</listOfReactants>
				<listOfProducts>
					<speciesReference species="B-cpd"/>
				</listOfProducts>
			</reaction>
		</listOfReactions>
	</model>
</sbml>